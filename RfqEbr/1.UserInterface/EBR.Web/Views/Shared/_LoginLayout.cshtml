
@using EBR.Web.Services
@{
    var pathImages = Url.Content("~/Images/");
    var parthIcon = pathImages + "YMTHolding.png";
    var baseurl = Request.Url.GetLeftPart(UriPartial.Authority) + Url.Content("~");
}
<!DOCTYPE html >

<html ng-app="MainApp">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="X-UA-Compatible" content="IE=10; IE=9; IE=8; IE=7; IE=EDGE" />
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="mobile-web-app-capable" content="yes">
    <title>YMT-GROUP</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Fav and touch icons -->
    <link href="@(parthIcon)" rel="shortcut icon" type="image/x-icon" />
    <link rel="icon" href="@(parthIcon)" sizes="32x32" />
    <link rel="icon" href="@(parthIcon)" sizes="48x48" />
    <link rel="icon" href="@(parthIcon)" sizes="64x64" />

    <link rel="shortcut icon" href="~/Images/YMTHolding.png">


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")

</head>

<body class="hold-transition skin-green layout-top-nav fixed" ng-app="MainApp">
    <div class="wrapper">
        <div class="content-wrapper">
            <section class="content ng-cloak">
                @RenderBody()
            </section>
        </div>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/addon")
    @Scripts.Render("~/bundles/app")

    <script>
        window.baseUrl = "@baseurl";
        window.autherize = "@Url.Action("Index", "Home")";
        Icon = "@parthIcon";
        //$(document).ready(function () {
 
        //    $('.select2').select2();
        //})
        //$(function () {

        //})

    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
